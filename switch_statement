#include <stdio.h>
int main() {
	int n = 0, i;
	for(i = 0; i < 3; i++) {
		switch(i) {
		    case 0:
			    n -= i;
		    case 1:
			    n += i;
			 //   break;
		    case 2:
			    n *= i;
       break;
		    default:
			    n += 5;
                break;
		}
	}
	printf("%d %d", n, i);
	return 0;
}
